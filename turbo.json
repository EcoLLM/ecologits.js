{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["es/**"]
    },
    "dev": {
      "dependsOn": ["libs#build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
